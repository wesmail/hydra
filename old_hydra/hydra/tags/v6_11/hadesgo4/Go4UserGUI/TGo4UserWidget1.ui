<!DOCTYPE UI><UI version="3.0" stdsetdef="1">
<class>TGo4UserWidget1</class>
<comment>An example of adding user gui's to the Go4 </comment>
<author>Mohammad Al-Turany</author>
<widget class="QWidget">
    <property name="name">
        <cstring>TGo4UserWidget1</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>396</width>
            <height>401</height>
        </rect>
    </property>
    <property name="sizePolicy">
        <sizepolicy>
            <hsizetype>7</hsizetype>
            <vsizetype>7</vsizetype>
            <horstretch>0</horstretch>
            <verstretch>0</verstretch>
        </sizepolicy>
    </property>
    <property name="caption">
        <string>TGo4UserWidget1</string>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>11</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QLabel" row="0" column="0" rowspan="1" colspan="2">
            <property name="name">
                <cstring>TextLabel1</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>7</hsizetype>
                    <vsizetype>5</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string>This is A QWidget with the TQRootCanvas, drag Histograms from the Go4 Browser and drop them to the Canvas below to draw them.</string>
            </property>
            <property name="textFormat">
                <enum>RichText</enum>
            </property>
        </widget>
        <widget class="TQRootCanvas" row="1" column="0" rowspan="1" colspan="2">
            <property name="name">
                <cstring>RootCanvas4</cstring>
            </property>
        </widget>
        <widget class="QLCDNumber" row="2" column="1">
            <property name="name">
                <cstring>LCDNumber2</cstring>
            </property>
        </widget>
        <widget class="QLCDNumber" row="2" column="0">
            <property name="name">
                <cstring>LCDNumber1</cstring>
            </property>
        </widget>
    </grid>
</widget>
<customwidgets>
    <customwidget>
        <class>TQRootCanvas</class>
        <header location="local">/u/shower/go4/go41.1/qtroot2.0/tqrootcanvas.h</header>
        <sizehint>
            <width>200</width>
            <height>300</height>
        </sizehint>
        <container>0</container>
        <sizepolicy>
            <hordata>7</hordata>
            <verdata>7</verdata>
            <horstretch>0</horstretch>
            <verstretch>0</verstretch>
        </sizepolicy>
        <pixmap>image0</pixmap>
    </customwidget>
</customwidgets>
<images>
    <image name="image0">
        <data format="XPM.GZ" length="1241"></data>
    </image>
</images>
<layoutdefaults spacing="6" margin="11"/>
</UI>
